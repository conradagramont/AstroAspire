{
  "resources": {
    "api": {
      "type": "project.v0",
      "path": "../AstroAspire.API/AstroAspire.API.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "services__frontend__http__0": "{frontend.bindings.http.url}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        }
      }
    },
    "apiBasePath": {
      "type": "parameter.v0",
      "value": "{apiBasePath.inputs.value}",
      "inputs": {
        "value": {
          "type": "string"
        }
      }
    },
    "frontend": {
      "type": "dockerfile.v0",
      "path": "../AstroFrontend/Dockerfile",
      "context": "../AstroFrontend",
      "env": {
        "NODE_ENV": "development",
        "services__api__http__0": "{api.bindings.http.url}",
        "services__api__https__0": "{api.bindings.https.url}",
        "apiBasePath": "{apiBasePath.value}",
        "BROWSER": "none",
        "PORT": "{frontend.bindings.http.targetPort}",
        "NODE_TLS_REJECT_UNAUTHORIZED": "0"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 8000,
          "external": true
        }
      }
    }
  }
}